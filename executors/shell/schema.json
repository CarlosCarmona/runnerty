{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "shell",
  "definitions": {
    "config":
      { "type": "object",
        "required": ["id","type"],
        "properties":{
          "id": {"type": "string"},
          "type": {"type": "string", "pattern": "\\bshell\\b"}
      }
    },
    "params":
    { "type": "object",
      "required": ["id","command"],
      "properties":{
        "id": {"type": "string"},
        "command": {"type": "string"}
      }
    }
  }
}