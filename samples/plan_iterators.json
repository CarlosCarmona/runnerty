{
  "chains":[
    {
      "id":"CHAIN_TEST_ITERABLE_OBJECT",
      "name":"CADENA DE PRUEBA ITERABLE",
      "iterable":"parallel",
      "depends_chains":[{"chain_id":"CHAIN_UNO","process_id":"PROC_LAUNCHER"}],
      "input":[{"MY_VAR_ID":"id"},{"MY_VAR_NAME":"text"}],
      "events_iterations":{
        "on_start":{
          "notifications":[
            {
              "id":"slack_default",
              "bot_emoji": ":rabbit2:",
              "channel": "mlm",
              "message":" ----> [ITER-I] INICIO DE LA CADENA ITERADA :CHAIN_ID CON :MY_VAR_ID Y :MY_VAR_NAME"
            }
          ]
        },"on_end":{
          "notifications":[
            {
              "id":"slack_default",
              "bot_emoji": ":rabbit2:",
              "channel": "mlm",
              "message":" ----> [ITER-F] FIN DE LA CADENA ITERADA :CHAIN_ID CON :MY_VAR_ID Y :MY_VAR_NAME"
            }
          ]
        }
      },
      "events":{
        "on_start":{
          "notifications":[
            {
              "id":"slack_default",
              "bot_emoji": ":rabbit2:",
              "channel": "mlm",
              "message":"[INICIO] INICIO DE LAS ITERACIONES :CHAIN_ID ------------------------->"
            }
          ]
        },"on_end":{
          "notifications":[
            {
              "id":"slack_default",
              "bot_emoji": ":rabbit2:",
              "channel": "mlm",
              "message":"[FIN] FIN DE LAS ITERACIONES :CHAIN_ID -------------------------<"
            }
          ]
        }
      },
      "processes":[{
        "id":"PROCCESS_ITERABLE",
        "name":"Concatenador de CSVs",
        "depends_process":[],
        "exec":{"command":"/etc/runnerty/sleeper.sh"},
        "args": ["EL ID ES :MY_VAR_ID", " Y EL NOMBRE :MY_VAR_NAME"],
        "retries":0,
        "retry_delay":0,
        "end_chain_on_fail":true,
        "output":[{"file_name":"/etc/runnerty/test.log", "write":["[!] EJECUCIÓN :DD-:MM-:YY :HH::mm::ss -","PROCESS_EXEC_DB_RETURN\n :PROCESS_EXEC_DB_RETURN","PROCESS_EXEC_DB_AFFECTEDROWS:\n :PROCESS_EXEC_DB_AFFECTEDROWS"], "concat":true, "maxsize":"1mb"},
          {"file_name":"/etc/runnerty/RES_MYSQL.log", "write":[":PROCESS_EXEC_DB_RETURN"], "concat":false, "maxsize":"1mb"},
          {"file_name":"/etc/runnerty/RES_MYSQL.csv", "write":[":PROCESS_EXEC_DB_RETURN_CSV"], "concat":false, "maxsize":"1mb"}],
        "events":{
          "on_start":{
            "notifications":[
              {
                "id":"slack_default",
                "bot_emoji": ":rabbit2:",
                "channel": "mlm",
                "message":"  ------> [PROC-I] INICIO DEL PROCESO *:PROCESS_ID* DE LA CADENA ITERABLE :CHAIN_ID CON :MY_VAR_ID Y :MY_VAR_NAME"
              }
            ]
          },
          "on_fail":{
            "notifications":[
              {
                "id":"slack_default",
                "bot_emoji": ":see_no_evil:",
                "channel": "mlm",
                "message":"  ------> [PROC-E] ERROR EN EL PROCESO *:PROCESS_ID* DE LA CADENA ITERABLE :CHAIN_ID - :PROCESS_EXEC_ERR_RETURN / :PROCESS_EXEC_RETURN"
              }
            ]
          },
          "on_retry":{},
          "on_end":{
            "notifications":[
              {
                "id":"slack_default",
                "bot_emoji": ":rabbit2:",
                "channel": "mlm",
                "message":"  ------> [PROC-F] FIN DEL PROCESO *:PROCESS_ID* DE LA CADENA ITERABLE :CHAIN_ID"
              }
            ]
          },
          "on_waiting_dependencies":{},
          "on_exceeded_limited_time_end":{}
        }
      },
      {
        "id":"PROCCESS_ITERABLE_2",
        "name":"Concatenador de CSVs",
        "depends_process":["PROCCESS_ITERABLE"],
        "exec":{"command":"echo"},
        "args": [" > SEGUNDO PROCESO :MY_VAR_ID", " Y EL NOMBRE :MY_VAR_NAME"],
        "retries":0,
        "retry_delay":0,
        "end_chain_on_fail":true,
        "output":[{"file_name":"/etc/runnerty/test.log", "write":["[!] EJECUCIÓN :DD-:MM-:YY :HH::mm::ss -","PROCESS_EXEC_DB_RETURN\n :PROCESS_EXEC_DB_RETURN","PROCESS_EXEC_DB_AFFECTEDROWS:\n :PROCESS_EXEC_DB_AFFECTEDROWS"], "concat":true, "maxsize":"1mb"},
          {"file_name":"/etc/runnerty/RES_MYSQL.log", "write":[":PROCESS_EXEC_DB_RETURN"], "concat":false, "maxsize":"1mb"},
          {"file_name":"/etc/runnerty/RES_MYSQL.csv", "write":[":PROCESS_EXEC_DB_RETURN_CSV"], "concat":false, "maxsize":"1mb"}],
        "events":{
          "on_start":{
            "notifications":[
              {
                "id":"slack_default",
                "bot_emoji": ":rabbit2:",
                "channel": "mlm",
                "message":"  ------ 2 > [PROC-I] INICIO DEL 2 PROCESO *:PROCESS_ID* DE LA CADENA ITERABLE :CHAIN_ID CON :MY_VAR_ID Y :MY_VAR_NAME"
              }
            ]
          },
          "on_fail":{
            "notifications":[
              {
                "id":"slack_default",
                "bot_emoji": ":see_no_evil:",
                "channel": "mlm",
                "message":"  ------ 2 > [PROC-E] ERROR EN EL 2 PROCESO *:PROCESS_ID* DE LA CADENA ITERABLE :CHAIN_ID - :PROCESS_EXEC_ERR_RETURN / :PROCESS_EXEC_RETURN"
              }
            ]
          },
          "on_retry":{},
          "on_end":{},
          "on_waiting_dependencies":{},
          "on_exceeded_limited_time_end":{}
        }
      }
      ]
    }
  ]
}