    "cz-conventional-changelog": "^2.0.0",
    "eslint": "^4.14.0",
    "eslint-plugin-jest": "^21.5.0",
    "jest": "^20.0.4",
    "jest-cli": "^20.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/loadConfigSection.js"
    ],
    "coverageThreshold": {
